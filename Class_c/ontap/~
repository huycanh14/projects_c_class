/*
	Viet chuong trinh nhap so nguyen duong n, sau do chen chu so nho nhat vao sau cac chu so chan
*/
#include <stdio.h>
#include <conio.h>
#include <stdlib.h>
main()
{
	int i, j, n;
	int *a;
	do
	{
		printf("Nhap so phan tu cua mang, n = ");
		scanf("%d", &n);
	}while(n<=0);
	
	a = (int*)calloc(n, sizeof(int));
	return 0;
}

